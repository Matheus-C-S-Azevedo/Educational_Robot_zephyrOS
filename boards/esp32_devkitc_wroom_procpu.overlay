/ {
    aliases {
        led0 = &user_led;
        sw0 = &user_button;
    };

    leds {
        compatible = "gpio-leds";
        user_led: led_0 {
            gpios = <&gpio0 13 GPIO_ACTIVE_HIGH>;
        };
    };

    buttons {
        compatible = "gpio-keys";
        user_button: button_0 {
            gpios = <&gpio0 4 GPIO_ACTIVE_HIGH>;
        };
    };
};

&wifi {
	status = "okay";
};